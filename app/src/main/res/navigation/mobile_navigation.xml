<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_cabinet">

    <fragment
        android:id="@+id/nav_cabinet"
        android:name="com.e.navigation_drawer_practice.ui.cabinet.CabinetFragment"
        android:label="My cabinet"
        tools:layout="@layout/fragment_cabinet" />

    <fragment
    android:id="@+id/nav_messages"
    android:name="com.e.navigation_drawer_practice.ui.messages.MessagesFragment"
    android:label="My chat"
    tools:layout="@layout/fragment_messages" />


    <fragment
        android:id="@+id/nav_home"
        android:name="com.e.navigation_drawer_practice.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home">
    </fragment>

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.e.navigation_drawer_practice.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.e.navigation_drawer_practice.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />
</navigation>